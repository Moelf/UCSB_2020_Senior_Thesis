@article{particle_flow,
	title = {Particle-flow reconstruction and global event description with the {CMS} detector},
	volume = {12},
	issn = {1748-0221},
	url = {http://arxiv.org/abs/1706.04965},
	doi = {10.1088/1748-0221/12/10/P10003},
	abstract = {The {CMS} apparatus was identified, a few years before the start of the {LHC} operation at {CERN}, to feature properties well suited to particle-flow ({PF}) reconstruction: a highly-segmented tracker, a fine-grained electromagnetic calorimeter, a hermetic hadron calorimeter, a strong magnetic field, and an excellent muon spectrometer. A fully-fledged {PF} reconstruction algorithm tuned to the {CMS} detector was therefore developed and has been consistently used in physics analyses for the first time at a hadron collider. For each collision, the comprehensive list of final-state particles identified and reconstructed by the algorithm provides a global event description that leads to unprecedented {CMS} performance for jet and hadronic tau decay reconstruction, missing transverse momentum determination, and electron and muon identification. This approach also allows particles from pileup interactions to be identified and enables efficient pileup mitigation methods. The data collected by {CMS} at a centre-of-mass energy of 8 {TeV} show excellent agreement with the simulation and confirm the superior {PF} performance at least up to an average of 20 pileup interactions.},
	pages = {P10003--P10003},
	number = {10},
	journaltitle = {Journal of Instrumentation},
	shortjournal = {J. Inst.},
	author = {{CMS Collaboration}},
	urldate = {2020-06-05},
	date = {2017-10-06},
	eprinttype = {arxiv},
	eprint = {1706.04965},
	keywords = {High Energy Physics - Experiment, Physics - Instrumentation and Detectors}
}
    
@article{offshell_theory1,
	title = {Inadequacy of zero-width approximation for a light Higgs boson signal},
	volume = {2012},
	issn = {1029-8479},
	url = {https://doi.org/10.1007/JHEP08(2012)116},
	doi = {10.1007/JHEP08(2012)116},
	abstract = {In the Higgs search at the {LHC}, a light Higgs boson \$ {\textbackslash}left( \{\{115\}{\textbackslash},\{{\textbackslash}text\{{GeV}\}\} {\textbackslash}lesssim \{M\_\{{\textbackslash}text\{H\}\}\} {\textbackslash}lesssim \{13\}0{\textbackslash},\{{\textbackslash}text\{{GeV}\}\}\} {\textbackslash}right) \$is not excluded by experimental data. In this mass range, the width of the Standard Model Higgs boson is more than four orders of magnitude smaller than its mass. The zero-width approximation is hence expected to be an excellent approximation. We show that this is not always the case. The inclusion of off-shell contributions is essential to obtain an accurate Higgs signal normalisation at the 1\% precision level. For gg (→ H) → {VV}, V = W,Z, \$ {\textbackslash}mathcal\{O\} \$(10 \%) corrections occur due to an enhanced Higgs signal in the region {MV} V{\textgreater} 2 {MV}, where also sizable Higgs-continuum interference occurs. We discuss how experimental selection cuts can be used to exclude this region in search channels where the Higgs invariant mass cannot be reconstructed. We note that the H → V V decay modes in weak boson fusion are similarly affected.},
	pages = {116},
	number = {8},
	journaltitle = {Journal of High Energy Physics},
	shortjournal = {J. High Energ. Phys.},
	author = {Kauer, Nikolas and Passarino, Giampiero},
	urldate = {2020-06-05},
	date = {2012-08-23},
	langid = {english},
	file = {Submitted Version:/home/akako/Zotero/storage/FPM8VBZ3/Kauer and Passarino - 2012 - Inadequacy of zero-width approximation for a light.pdf:application/pdf}
}

@article{offshell_theory2,
	title = {Bounding the Higgs width at the {LHC} using full analytic results for gg → e−e+μ−μ+},
	volume = {2014},
	issn = {1029-8479},
	url = {https://doi.org/10.1007/JHEP04(2014)060},
	doi = {10.1007/JHEP04(2014)060},
	abstract = {We revisit the hadronic production of the four-lepton final state, e−e+μ−μ+, through the fusion of initial state gluons. This process is mediated by loops of quarks and we provide first full analytic results for helicity amplitudes that account for both the effects of the quark mass in the loop and off-shell vector bosons. The analytic results have been implemented in the Monte Carlo program {MCFM} and are both fast, and numerically stable in the region of low Z transverse momentum. We use our results to study the interference between Higgs-mediated and continuum production of four-lepton final states, which is necessary in order to obtain accurate theoretical predictions outside the Higgs resonance region. We have confirmed and extended a recent analysis of Caola and Melnikov that proposes to use a measurement of the off-shell region to constrain the total width of the Higgs boson. Using a simple cut-and-count method, existing {LHC} data should bound the width at the level of 25–45 times the Standard Model expectation. We investigate the power of using a matrix element method to construct a kinematic discriminant to sharpen the constraint. In our analysis the bound on the Higgs width is improved by a factor of about 1.6 using a simple cut on the {MEM} discriminant, compared to an invariant mass cut m4l{\textgreater} 300 {GeV}.},
	pages = {60},
	number = {4},
	journaltitle = {Journal of High Energy Physics},
	shortjournal = {J. High Energ. Phys.},
	author = {Campbell, John M. and Ellis, R. Keith and Williams, Ciaran},
	urldate = {2020-06-05},
	date = {2014-04-09},
	langid = {english},
	file = {Springer Full Text PDF:/home/akako/Zotero/storage/NBEGAR5Q/Campbell et al. - 2014 - Bounding the Higgs width at the LHC using full ana.pdf:application/pdf}
}

@book{offshell_poc,
	title = {Measurements of Higgs Boson Properties in Proton-Proton Collisions at $\sqrt{s} = 7$, 8 and 13 {TeV} at the {CERN} Large Hadron Collider},
	isbn = {978-3-030-25473-5},
	url = {https://www.springer.com/us/book/9783030254735},
	series = {Springer Theses},
	abstract = {This thesis documents the measurement of lifetime, width, mass, and couplings to two electroweak bosons of the recently-discovered Higgs boson using data from the {CMS} experiment at the Large Hadron Collider. Both on-shell (at the mass of around 125 {GeV}) and off-shell (above 200 {GeV}) Higgs boson production is studied and an excess of off-shell production with significance above two standard deviations is observed for the first time. The latter is a qualitative new way to study the Higgs field, responsible for generation of mass of all the known elementary particles. In addition, phenomenological tools have been developed with the Monte Carlo event generator and matrix element techniques for an optional analysis of {LHC} data. Optimization of the {CMS} data with careful alignment of the silicon tracker is also presented.},
	publisher = {Springer International Publishing},
	author = {Sarica, Ulascan},
