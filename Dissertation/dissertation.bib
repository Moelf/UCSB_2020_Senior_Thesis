	doi = {10.1007/JHEP08(2016)045},
	abstract = {Combined {ATLAS} and {CMS} measurements of the Higgs boson production and decay rates, as well as constraints on its couplings to vector bosons and fermions, are presented. The combination is based on the analysis of five production processes, namely gluon fusion, vector boson fusion, and associated production with a \$W\$ or a \$Z\$ boson or a pair of top quarks, and of the six decay modes \$H {\textbackslash}to {ZZ}, {WW}\$, \${\textbackslash}gamma{\textbackslash}gamma, {\textbackslash}tau{\textbackslash}tau, bb\$, and \${\textbackslash}mu{\textbackslash}mu\$. All results are reported assuming a value of 125.09 {GeV} for the Higgs boson mass, the result of the combined measurement by the {ATLAS} and {CMS} experiments. The analysis uses the {CERN} {LHC} proton--proton collision data recorded by the {ATLAS} and {CMS} experiments in 2011 and 2012, corresponding to integrated luminosities per experiment of approximately 5 fb\${\textasciicircum}\{-1\}\$ at \${\textbackslash}sqrt\{s\}=7\$ {TeV} and 20 fb\${\textasciicircum}\{-1\}\$ at \${\textbackslash}sqrt\{s\} = 8\$ {TeV}. The Higgs boson production and decay rates measured by the two experiments are combined within the context of three generic parameterisations: two based on cross sections and branching fractions, and one on ratios of coupling modifiers. Several interpretations of the measurements with more model-dependent parameterisations are also given. The combined signal yield relative to the Standard Model prediction is measured to be 1.09 \${\textbackslash}pm\$ 0.11. The combined measurements lead to observed significances for the vector boson fusion production process and for the \$H {\textbackslash}to {\textbackslash}tau{\textbackslash}tau\$ decay of \$5.4\$ and \$5.5\$ standard deviations, respectively. The data are consistent with the Standard Model predictions for all parameterisations considered.},
	pages = {45},
	number = {8},
	journaltitle = {Journal of High Energy Physics},
	shortjournal = {J. High Energ. Phys.},
	author = {{ATLAS} and Collaborations, C. M. S.},
	urldate = {2020-06-08},
	date = {2016-08},
	eprinttype = {arxiv},
	eprint = {1606.02266},
	keywords = {High Energy Physics - Experiment},
	file = {arXiv Fulltext PDF:/home/akako/Zotero/storage/9YJMFJGR/ATLAS and Collaborations - 2016 - Measurements of the Higgs boson production and dec.pdf:application/pdf;arXiv.org Snapshot:/home/akako/Zotero/storage/YF9AU5K5/1606.html:text/html}
}

