@article{k_factor,
	title = {What is the Real K Factor?},
	abstract = {The theoretical K factor, describing the difference between the leading and higher order cross sections, has no precise deﬁnition. The deﬁnition is sensitive to the order of the ﬁt to the parton densities and the number of loops at which αs is evaluated. We describe alternate ways to deﬁne the K factor and show how the deﬁnition affects its magnitude and shape for examples of hadroproduction of W + bosons, Drell-Yan lepton pairs, and heavy quarks. We discuss which deﬁnition is appropriate under certain circumstances.},
